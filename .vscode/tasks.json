{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build"
		},
		{
			"label": "cargo run --release",
			"type": "shell",
			"command": "cargo",
			"args": [
				"run",
				"--release",
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}